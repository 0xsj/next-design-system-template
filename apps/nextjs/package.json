{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TAMAGUI_TARGET=web && yarn with-env next dev",
    "build": "TAMAGUI_TARGET=web && yarn with-env next build",
    "with-env": "dotenv -e ../../.env --",
    "clean": "rm -rf .next .turbo node_modules .tamagui",
    "start": "next start",
    "serve": "NODE_ENV=production && next start --port 8151",
    "lint": "next lint",
    "export": "next export",
    "dev:tauri": "yarn tauri dev",
    "build:tauri": "yarn tauri build"
  },
  "dependencies": {
    "@tamagui/next-theme": "latest",
    "app": "*",
    "dotenv-cli": "^6.0.0",
    "next": "13.4.4",
    "raf": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.8",
    "react-native-web": "^0.18.12",
    "react-native-web-lite": "latest",
    "vercel": "latest"
  },
  "devDependencies": {
    "@tamagui/next-plugin": "latest",
    "@tauri-apps/cli": "^1.2.3",
    "@types/node": "^18.6.4",
    "critters": "^0.0.16",
    "eslint-config-next": "^13.0.4",
    "next-images": "^1.8.4"
  }
}
